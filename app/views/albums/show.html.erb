<div class="album-info">
    <div class="album-info-jaket">
        <%= image_tag(@album.img_path, class: "album-jaket-img") %>
    </div>
    <div class="album-info-text">
        <ul>
            <li><h1><%= @album.name %></h1></li>
        </ul>
    </div>
</div>

<div class="album-musics" >
    <% @musics.each do |music| %>
        <div class="album-music-track"><%= link_to(music.track, "/musics/#{music.id}") %></div>
        <div class="album-music-track"><%= link_to(music.name, "/musics/#{music.id}") %></div>
        <div class="album-music-track"><%= link_to(music.artist, "/musics/#{music.id}") %></div>
    <% end %>
</div>